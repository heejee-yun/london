<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.augfw.infra.modules.member.MemberMapper">

    <resultMap id="resultMapObj" type="com.augfw.infra.modules.member.Member"></resultMap>
	
	<select id="selectList" resultMap="resultMapObj">
		SELECT
			a.seq
			,a.id
			,a.name
			,a.mobile
			,a.email
			,a.dob
		FROM member a
		WHERE 1=1
		AND resignny = 0

<!-- 		SELECT
			a.*
		FROM member a
		WHERE 1=1
			AND resignny = 0 -->
	</select>

</mapper>	